[
  {
    "id": "redeploy-webhook",
    "execute-command": "/var/webhook/redeploy.sh",
    "command-working-directory": "/var/webhook",
    "trigger-rules":
    {
      "and":
      [
        {
          "match":
          {
            "type": "payload-hash-sha1",
            "secret": "SECRET-KEY-HERE",
            "parameter":
            {
              "source": "header",
              "name": "X-Hub-Signature"
            }
          }
        },
        {
          "match":
          {
            "type": "value",
            "value": "refs/heads/master",
            "parameter":
            {
              "source": "payload",
              "name": "ref"
            }
          }
        }
      ]
    }
  }
]